{
  "name": "@dkstore/queue",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Queue package for DkStore.",
  "sideEffects": false,
  "exports": {
    "./configs": {
      "types": "./dist/configs.d.ts",
      "import": "./dist/configs.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./bullmq": {
      "types": "./dist/bullmq.d.ts",
      "import": "./dist/bullmq.js"
    },
    "./*": {
      "types": "./dist/queues/*.d.ts",
      "import": "./dist/queues/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "clean": "git clean -xdf .turbo dist node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dkstore/env": "workspace:*",
    "bullmq": "^5.44.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dkshs/eslint-config": "4.5.2",
    "@dkstore/tsconfig": "workspace:*",
    "@rslib/core": "^0.6.0",
    "@types/node": "^22.13.13",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20"
  }
}
{
  "name": "@dkstore/env",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Environment variables for DkStore.",
  "sideEffects": false,
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./presets/*": {
      "types": "./dist/presets/*.d.ts",
      "import": "./dist/presets/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "clean": "git clean -xdf dist .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.12.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dkstore/tsconfig": "workspace:*",
    "@ncontiero/eslint-config": "5.1.1",
    "@rslib/core": "^0.6.3",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
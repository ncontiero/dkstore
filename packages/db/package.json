{
  "name": "@dkstore/db",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Database package for DkStore.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "concurrently --kill-others --names \"db-studio,db-build\" -c \"black,blue\" \"pnpm db:studio\" \"rslib build --watch\"",
    "clean": "git clean -xdf .turbo dist node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "pnpm db:generate"
  },
  "dependencies": {
    "@dkstore/env": "workspace:*",
    "@prisma/client": "6.6.0"
  },
  "devDependencies": {
    "@dkstore/tsconfig": "workspace:*",
    "@ncontiero/eslint-config": "5.1.1",
    "@rslib/core": "^0.6.2",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "prisma": "6.6.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}